#pragma experiment("TRAITS")

import Electrical
import is_cable

module RibbonCable4:
    """4-conductor ribbon cable connecting two boards."""
    trait is_cable

    side_a = new Electrical[4]
    side_b = new Electrical[4]

    side_a[0] ~ side_b[0]
    side_a[1] ~ side_b[1]
    side_a[2] ~ side_b[2]
    side_a[3] ~ side_b[3]
