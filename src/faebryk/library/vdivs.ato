import Capacitor, ResistorVoltageDivider, ElectricPower, ElectricSignal

module VDiv:
    power = new ElectricPower
    out = new ElectricSignal
    vdiv = new ResistorVoltageDivider
    power ~ vdiv.power
    out ~ vdiv.output

    v_in: voltage
    v_out: voltage
    i_q: current

    v_in = vdiv.v_in
    v_out = vdiv.v_out
    i_q = vdiv.max_current

