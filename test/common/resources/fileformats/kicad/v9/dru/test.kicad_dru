(version 1)
(rule "Min Clearance"
	(severity error)
	(constraint clearance
		(min 0.4mm)
	)
)
(rule "Track Width"
	(severity warning)
	(constraint track_width
		(min 0.15mm)
		(opt 0.25mm)
		(max 5mm)
	)
)
(rule "Diff Pair Width"
	(condition "A.NetClass == 'Diff'")
	(constraint track_width
		(min 0.1mm)
		(opt 0.1mm)
		(max 0.1mm)
	)
	(constraint diff_pair_gap
		(min 0.1mm)
		(opt 0.1mm)
		(max 0.1mm)
	)
)
(rule "Silk Clearance"
	(severity ignore)
	(layer "F.SilkS")
	(constraint silk_clearance
		(min 0.15mm)
	)
)
